{
  "rules": {
    "indent": [
      2,
      "tab"
    ],
    "strict": 0,
    "no-console": 0,
    "quotes": [
      2,
      "double"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-useless-escape": "off"
  },
  "env": {
    "browser": true,
    "node":true
  },
  "extends": "eslint:recommended"
}